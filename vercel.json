{
  "buildCommand": "npm run build",
  "devCommand": "next dev",
  "installCommand": "npm install && pip install -r requirements.txt",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "env": {
    "OPENAI_API_KEY": "${OPENAI_API_KEY}"
  },
  "crons": [
    {
      "path": "/api/cron/scrape",
      "schedule": "0 */6 * * *"
    }
  ]
}

